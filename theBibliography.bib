  keywords = {Animals, Brain/*drug effects/*metabolism, Designer Drugs/*therapeutic use, Humans, Learning/drug effects/physiology, Memory/drug effects, Motivation/drug effects, Neurosciences/*methods, Reward, Signal Transduction},
  language = {eng},
  url      = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4792665/},
}

