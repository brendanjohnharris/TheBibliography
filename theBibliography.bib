  groups           = {Nonstationary Parameter Estimation},
  issn             = {1948-3457},
  keywords         = {control system synthesis, convex programming, identification, learning (artificial intelligence), linear systems, nonlinear control systems, state-space methods, time-varying systems, trend filtering, system identification, identification methods, time-varying dynamical models, cost function, continuous change, discontinuous changes, model coefficients, finite set, time instances, model parameters, identification problems, convex optimization problems, ARX models, state-space models, time-varying parameters, jump-linear systems, nonsmooth friction, smooth nonlinear system, LTV dynamical models, discontinuous time evolution, trajectory centric reinforcement learning, Heuristic algorithms, Convex functions, Market research, Data models, Cost function, Kalman filters},
  modificationdate = {2021-05-02T17:04:47},
