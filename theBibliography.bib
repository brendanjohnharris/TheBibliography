@Article{Koch2015,
  author           = {Koch, Stefan P. and Hägele, Claudia and Haynes, John-Dylan and Heinz, Andreas and Schlagenhauf, Florian and Sterzer, Philipp},
  journal          = {PLOS ONE},
  title            = {Diagnostic Classification of Schizophrenia Patients on the Basis of Regional Reward-Related fMRI Signal Patterns},
  year             = {2015},
