@Article{Lainscsek2015,
  author           = {Lainscsek, Claudia and Weyhenmeyer, Jonathan and Sejnowski, Terrence J. and Letellier, Christophe},
  journal          = {Chaos, solitons, and fractals},
  title            = {Discovering independent parameters in complex dynamical systems},
  year             = {2015},
  issn             = {0960-0779},
  month            = jul,
  pages            = {182--189},
  volume           = {76},
  abstract         = {The transformation of a nonlinear dynamical system into a standard form by using one of its variables and its successive derivatives can be used to identify the relationships that may exist between the parameters of the original system such as the subset of the parameter space over which the dynamics is left invariant. We show how the size of the attractor or the time scale (the pseudo-period) can be varied without affecting the underlying dynamics. This is demonstrated for the Rössler and the Lorenz systems. We also consider the case when two Rössler systems are unidirectionally coupled and when a Lorenz system is driven by a Rössler system. In both cases, the dynamics of the coupled system is affected.},
  doi              = {10.1016/j.chaos.2015.04.001},
  file             = {:Lainscsek2015 - Discovering Independent Parameters in Complex Dynamical Systems.pdf:PDF},
  groups           = {Nonstationary Parameter Estimation},
  modificationdate = {2021-05-11T12:36:02},
  pmcid            = {PMC4429610},
  pmid             = {25983399},
  url              = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4429610/},
  urldate          = {2021-05-11},
}

@Article{Li2014,
  author           = {Li, Chunbiao and Sprott, Julien Clinton and Thio, Wesley and Zhu, Huanqiang},
  journal          = {IEEE Transactions on Circuits and Systems II: Express Briefs},
  title            = {A {New} {Piecewise} {Linear} {Hyperchaotic} {Circuit}},
  year             = {2014},
  issn             = {1558-3791},
  month            = dec,
  number           = {12},
  pages            = {977--981},
  volume           = {61},
  abstract         = {When the polarity information in diffusionless Lorenz equations is preserved or removed, a new piecewise linear hyperchaotic system results with only signum and absolute-value nonlinearities. Dynamical equations have seven terms without any quadratic or higher order polynomials and, to our knowledge, are the simplest hyperchaotic system. Therefore, a relatively simple hyperchaotic circuit using diodes is constructed. The circuit requires no multipliers or inductors, as are present in other hyperchaotic circuits, and it has not been previously reported.},
  doi              = {10.1109/TCSII.2014.2356912},
  file             = {:Li2014 - A New Piecewise Linear Hyperchaotic Circuit.pdf:PDF},
  groups           = {Systems},
  keywords         = {Bifurcation, Inductors, Chaotic communication, Oscillators, Hyperchaos, piecewise linear, signum nonlinearity},
  modificationdate = {2021-05-09T14:44:16},
}

@Article{Alsing1996,
  author           = {Alsing, P. M. and Kovanis, V. and Gavrielides, A. and Erneux, T.},
  title            = {Lang and {Kobayashi} phase equation},
  number           = {6},
  pages            = {4429--4434},
  volume           = {53},
  abstract         = {Lang and Kobayashi equations for a semiconductor laser subject to optical feedback are investigated by using asymptotic methods. Our analysis is based on the values of two key parameters, namely, the small ratio of the photon and carrier lifetimes and the relatively large value of the linewidth enhancement factor. For low feedback levels, we derive a third-order delay-differential equation for the phase of the laser field. We then show analytically and numerically that this equation admits coexisting branches of stable periodic solutions that appear at different and almost constant amplitudes. These amplitudes are proportional to the roots of the Bessel function J1(x). The bifurcation diagram of the phase equation is in good agreement with the numerical bifurcation diagram of the original Lang and Kobayashi equations. We interpret the onset of the periodic solutions as the emergence of a new set of external cavity modes with a more complicated time dependence. © 1996 The American Physical Society.},
  date             = {1996-06},
  doi              = {10.1103/PhysRevA.53.4429},
  file             = {:Alsing1996 - Lang and Kobayashi Phase Equation.pdf:PDF},
  groups           = {Lasers},
  journaltitle     = {Physical Review A},
  modificationdate = {2021-05-27T10:56:34},
  publisher        = {American Physical Society},
  urldate          = {2021-05-05},
}

@Article{Roy1992,
