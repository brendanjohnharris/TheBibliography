  copyright        = {Copyright © 2019, American Association for the Advancement of Science. http://www.sciencemag.org/about/science-licenses-journal-article-reuseThis is an article distributed under the terms of the Science Journals Default License.},
  doi              = {10.1126/science.aav7893},
  file             = {:Stringer2019 - Spontaneous Behaviors Drive Multidimensional, Brainwide Activity.pdf:PDF},
  groups           = {Neuro},
  language         = {en},
  modificationdate = {2021-05-02T17:04:47},
  pmid             = {31000656},
  priority         = {prio1},
  publisher        = {American Association for the Advancement of Science},
  url              = {https://science.sciencemag.org/content/364/6437/eaav7893},
